@font-face {
  font-family: 'LineIcons';

  src: url('60e5f82669d1e0c6cc24cbe019313a1f');

  src: url('60e5f82669d1e0c6cc24cbe019313a1f') format('embedded-opentype'), url('7ce365eb9949f2e44e1cb714a45709a0') format('woff2'), url('0f12a0df3a2e2a829413cc26b61a3251') format('woff'), url('c7766e72f3496cdf00142ac0f084f52a') format('truetype'), url('353e4a1980a1372f269ec10b6cd9f9aa') format('svg');

  font-weight: normal;

  font-style: normal;
}

/* --------------------------------
  
  icons
  
  -------------------------------- */

.lni {
  display: inline-block;
  font: normal normal normal 32px/1 'LineIcons';
  speak: none;
  text-transform: none;
  /* Better Font Rendering */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

